{
  "expo": {
    "name": "GLU-MAD-ReactNativeExpoExample",
    "description": "Grafisch Lyceum Utrecht - Mobile Application Development - React Native Expo example.",
    "slug": "expoProject",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.martijnkunstman.ReactNativeExoExample"
    }
  }
}
